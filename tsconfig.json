{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@constants": ["src/constants"],
      "@types": ["src/types"],
      "@navTypes": ["src/navTypes"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@store/*": ["src/store/*"],
      "@hooks/*": ["src/hooks/*"],
      "@utils/*": ["src/utils/*"],
      "@infrastructure/*": ["src/infrastructure/*"],
      "@data/*": ["src/data/*"],
      "@theme/*": ["src/theme/*"],
      "@assets/*": ["assets/*"]
    }
  }
}
